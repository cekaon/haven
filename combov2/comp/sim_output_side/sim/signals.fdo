proc blk_FL_BINDER { BASE } {
  add_wave "-noupdate -label CLK"    $BASE/CLK
  add_wave "-noupdate -label RESET"  $BASE/RESET

  add wave -divider "Input interfaces"
  add wave -divider "Input portout monitor ifc"
  add_wave "-noupdate -label fl_binder_in_pm_data -hex" $BASE/fl_binder_in_pm_data
  add_wave "-noupdate -label fl_binder_in_pm_rem" $BASE/fl_binder_in_pm_rem
  add_wave "-noupdate -label fl_binder_in_pm_src_rdy_n" $BASE/fl_binder_in_pm_src_rdy_n
  add_wave "-noupdate -label fl_binder_out_pm_dst_rdy_n" $BASE/fl_binder_out_pm_dst_rdy_n
  add_wave "-noupdate -label fl_binder_in_pm_sop_n" $BASE/fl_binder_in_pm_sop_n
  add_wave "-noupdate -label fl_binder_in_pm_eop_n" $BASE/fl_binder_in_pm_eop_n
  add_wave "-noupdate -label fl_binder_in_pm_sof_n" $BASE/fl_binder_in_pm_sof_n
  add_wave "-noupdate -label fl_binder_in_pm_eof_n" $BASE/fl_binder_in_pm_eof_n

  add wave -divider "Input register monitor ifc"
  add_wave "-noupdate -label fl_binder_in_rm_data -hex" $BASE/fl_binder_in_rm_data
  add_wave "-noupdate -label fl_binder_in_rm_rem" $BASE/fl_binder_in_rm_rem
  add_wave "-noupdate -label fl_binder_in_rm_src_rdy_n" $BASE/fl_binder_in_rm_src_rdy_n
  add_wave "-noupdate -label fl_binder_out_rm_dst_rdy_n" $BASE/fl_binder_out_rm_dst_rdy_n
  add_wave "-noupdate -label fl_binder_in_rm_sop_n" $BASE/fl_binder_in_rm_sop_n
  add_wave "-noupdate -label fl_binder_in_rm_eop_n" $BASE/fl_binder_in_rm_eop_n
  add_wave "-noupdate -label fl_binder_in_rm_sof_n" $BASE/fl_binder_in_rm_sof_n
  add_wave "-noupdate -label fl_binder_in_rm_eof_n" $BASE/fl_binder_in_rm_eof_n

  add wave -divider "Input memory monitor ifc"
  add_wave "-noupdate -label fl_binder_in_mm_data -hex" $BASE/fl_binder_in_mm_data
  add_wave "-noupdate -label fl_binder_in_mm_rem" $BASE/fl_binder_in_mm_rem
  add_wave "-noupdate -label fl_binder_in_mm_src_rdy_n" $BASE/fl_binder_in_mm_src_rdy_n
  add_wave "-noupdate -label fl_binder_out_mm_dst_rdy_n" $BASE/fl_binder_out_mm_dst_rdy_n
  add_wave "-noupdate -label fl_binder_in_mm_sop_n" $BASE/fl_binder_in_mm_sop_n
  add_wave "-noupdate -label fl_binder_in_mm_eop_n" $BASE/fl_binder_in_mm_eop_n
  add_wave "-noupdate -label fl_binder_in_mm_sof_n" $BASE/fl_binder_in_mm_sof_n
  add_wave "-noupdate -label fl_binder_in_mm_eof_n" $BASE/fl_binder_in_mm_eof_n


  add wave -divider "Output interface - fl"
  add_wave "-noupdate -label fl_binder_out_data -hex" $BASE/fl_binder_out_data
  add_wave "-noupdate -label fl_binder_out_rem" $BASE/fl_binder_out_rem
  add_wave "-noupdate -label fl_binder_out_src_rdy_n" $BASE/fl_binder_out_src_rdy_n
  add_wave "-noupdate -label fl_binder_in_dst_rdy_n" $BASE/fl_binder_in_dst_rdy_n
  add_wave "-noupdate -label fl_binder_out_sop_n" $BASE/fl_binder_out_sop_n
  add_wave "-noupdate -label fl_binder_out_eop_n" $BASE/fl_binder_out_eop_n
  add_wave "-noupdate -label fl_binder_out_sof_n" $BASE/fl_binder_out_sof_n
  add_wave "-noupdate -label fl_binder_out_eof_n" $BASE/fl_binder_out_eof_n

}

