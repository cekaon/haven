  add_wave "-noupdate -label memory_monitor_out_dbg" $BASE/memory_monitor_out_dbg
  add_wave "-noupdate -label memory_monitor_in_q0 -hex" $BASE/memory_monitor_in_q0
  add_wave "-noupdate -label memory_monitor_out_ra0 -hex" $BASE/memory_monitor_out_ra0
  add_wave "-noupdate -label memory_monitor_out_re0" $BASE/memory_monitor_out_re0
  add_wave "-noupdate -label memory_monitor_out_rsc0" $BASE/memory_monitor_out_rsc0
  add_wave "-noupdate -label memory_monitor_out_rsi0" $BASE/memory_monitor_out_rsi0


  add wave -divider "Output interface"
  add_wave "-noupdate -label memory_monitor_out_done" $BASE/memory_monitor_out_done
  add_wave "-noupdate -label memory_monitor_out_data -hex" $BASE/memory_monitor_out_data
  add_wave "-noupdate -label memory_monitor_out_rem -bin" $BASE/memory_monitor_out_rem
  add_wave "-noupdate -label memory_monitor_out_src_rdy_n" $BASE/memory_monitor_out_src_rdy_n
  add_wave "-noupdate -label memory_monitor_in_dst_rdy_n" $BASE/memory_monitor_in_dst_rdy_n
  add_wave "-noupdate -label memory_monitor_out_sop_n" $BASE/memory_monitor_out_sop_n
  add_wave "-noupdate -label memory_monitor_out_sof_n" $BASE/memory_monitor_out_sof_n
